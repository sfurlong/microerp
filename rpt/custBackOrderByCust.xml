   <Column>
      <Col_SQL_Name>unit_price</Col_SQL_Name>
      <Col_Heading>&lt;U&gt;Unit Price</Col_Heading>
   </Column>
   <Column>
      <Col_SQL_Name>(qty_ordered - qty_shipped) * unit_price</Col_SQL_Name>
      <Col_Heading>&lt;U&gt;Total</Col_Heading>
      <Display_Format>CURRENCY</Display_Format>
   </Column>
   <SQL_Where_Clause>where cust_order.id = cust_order_item.id and cust_order.locality = cust_order_item.locality and cust_order.customer_id like '?:0%' and (qty_ordered &gt; qty_shipped or qty_shipped is null) order by customer_name, item_id </SQL_Where_Clause>
