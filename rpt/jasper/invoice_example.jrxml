	<parameter name="invoice_id" isForPrompting="true" class="java.lang.Integer">
		<parameterDescription><![CDATA[Invoice ID]]></parameterDescription>
		<defaultValueExpression ><![CDATA[new Integer(28810)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT

  al.ledger_id, al.reason,

  /* unit details */
  CONCAT_WS('\n',
    u.name,
    u.street,
    CONCAT_WS(' ', u.city, u_st.description, u.postcode),
    CONCAT('Fax: ', u.fax)
  ) AS department,

  /* account details */
  a.name as name,

  /* account_invoice details */
  ai.unit_code AS unit_code,
  ai.internal_id AS invoice_no,
  ai.post_dtm AS post_date,
  ai.amount AS amount,
  ai.amount_due AS amount_due,
  ai.amount - ai.amount_due AS amount_paid,
  ai.due_date AS due_date,

  /* account_invoice_item details */
  aii.account_invoice_item_id AS item_id,
  aii.quantity AS item_quantity,
  aii.amount AS item_amount,
  aii.amount * aii.quantity AS item_total,
  aii.reason AS item_reason,
  aii.parent_id AS item_parent_id,
  aii.orig_amount AS item_orig_amount,
  aii.public_reason_b AS item_public_reason,
  c.title as item_title,
  CONCAT_WS(' ',
    s.firstname, s.lastname
  ) AS item_student_name,
  s.ce_num AS item_student_ce_num,
  co.start_date AS item_start_date,
  co.start_time AS item_start_time,

  /* account_address details */
  IF (a.account_type = 'Individual',
    CONCAT_WS('\n',
      CONCAT_WS(' ', am_sa.street1, am_sa.street2, am_sa.street3, am_sa.street4),
      CONCAT_WS(' ', am_sa.city, am_sa_st.description, am_sa.postcode, am_sa.country_code)
    ),
    CONCAT_WS('\n',
      CONCAT_WS(' ', aa.street1, aa.street2, aa.street3, aa.street4),
      CONCAT_WS(' ', aa.city, aa_st.description, aa.postcode, aa.country_code)
    )
  ) AS address,

  /* student info */
  CONCAT_WS(' ', s.prefix, s.firstname, s.lastname) AS student_name,
  s.ce_num AS ce_student_id,

  /* student_address info */
  CONCAT_WS('\n',
    CONCAT_WS(' ', aa.street1, aa.street2, aa.street3, aa.street4),
    CONCAT_WS(' ', aa.city, aa_st.description, aa.postcode, aa.country_code)
  ) AS student_address


FROM account_invoice ai

JOIN unit u
  ON (u.unit_code = ai.unit_code)
JOIN state u_st
  ON (u_st.state_id = u.state_id)

/* account and account address */
JOIN account a
  ON (a.account_id = ai.account_id)
LEFT JOIN account_address aa
  ON (aa.account_id = a.account_id)
LEFT JOIN state aa_st
  ON (aa_st.state_id = aa.state_id)


/* account_member and student_address */
LEFT JOIN account_member am
  ON (am.account_id = ai.account_id)
LEFT JOIN student_address am_sa
  ON (am_sa.student_id = am.student_id)
LEFT JOIN state am_sa_st
  ON (am_sa_st.state_id = am_sa.state_id)

/* based off account_invoice_item */
JOIN account_invoice_item aii
  ON (aii.account_invoice_id = ai.account_invoice_id)
JOIN registration r
  ON (r.registration_id = aii.registration_id)
JOIN student s
  ON (s.student_id = r.student_id)
JOIN course_offering co
  ON (co.course_offering_id = r.course_offering_id)
JOIN course c
  ON (c.course_id = co.course_id)
LEFT JOIN account_ledger al
  ON (al.account_invoice_item_id = aii.account_invoice_item_id)

WHERE ai.account_invoice_id = $P{invoice_id}
GROUP BY aii.account_invoice_item_id]]></queryString>

